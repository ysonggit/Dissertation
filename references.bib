@inproceedings{IslKanKha04,
  author       = {V. Isler and S. Kannan and S. Khanna},
  title        = {Locating and Capturing an Evader in a Polygonal
                 Environment},
  booktitle    = wafr,
  year         = {2004},
  keywords     = {pursuit},
  abstract     = {This paper contains two main results: First, we
                 revisit the well-known visibility based
                 pursuit-evasion problem and show that, in contrast to
                 deterministic strategies, a single pursuer can locate
                 an unpredictable evader in any simplyconnected
                 polygonal environment using a randomized strategy. The
                 evader can be arbitrarily faster than the pursuer and
                 it may know the position of the pursuer at all times
                 but it does not have prior knowledge of the random
                 decisions made by the pursuer. Second, using the
                 randomized algorithm together with the solution of a
                 known lion and man problem [12] as subroutines, we
                 present a strategy for two pursuers (one of which is
                 at least as fast as the evader) to quickly capture an
                 evader in a simply-connected polygonal environment.},
  owner        = {jokane},
