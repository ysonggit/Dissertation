@article{FarIoc+04,
  author       = {A. Farinelli and L. Iocchi and D. Nardi},
  title        = {Multi-Robot Systems: A classification focused on
                 coordination},
  journal      = tsmc,
  volume       = {34},
  pages        = {2015--2028},
  year         = {2004},
  abstract     = {Multi-Robot Systems (MRS) are, nowadays, an important
                 research area within Robotics and Artificial
                 Intelligence and a growing number of systems has been
                 recently presented in the literature. Since
                 application domains and tasks that are faced by MRS
                 are of increasing complexity, the ability of the
                 robots to cooperate can be regarded as a fundamental
                 feature. In this paper, we present a survey of the
                 recent work in the area by specifically examining the
                 forms of cooperation and coordination realized in the
                 MRS. In particular, we propose a new taxonomy for
                 classification of the approaches to coordination in
                 MRS and we describe some systems, which we consider
                 representative in our taxonomy. We finally discuss the
                 outcomes of our analysis and try to highlight future
                 trends of the research on MRS.},
  url          =
